    dct:isPartOf <https://natanael.arndt.xyz/bib/2024-3-workshop-digitale-lehre> ;
    dct:issued "2025-07-29"^^xsd:date ;
    dct:title "Digitale Selbstlern-Elemente für Grundlagen der Informatik" ;
    bibo:doi "10.33968/2025.01" .

